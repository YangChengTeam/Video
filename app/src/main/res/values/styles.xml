<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/gray_qian</item>
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--
colorPrimaryDark:标题栏背景色
colorPrimary:例如RecylerView滚动时候的阴影
colorAccent:主题色，如输入框，系统的自定义空间等
-->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <!--BaseAnimation-->
    <style name="NewQu.Animation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/screen_zoom_in</item>
        <item name="android:activityOpenExitAnimation">@anim/screen_zoom_out</item>
        <item name="android:activityCloseExitAnimation">@anim/screen_zoom_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/screen_zoom_in</item>
        <!--<item name="android:activityOpenEnterAnimation">@anim/slide_in_from_right</item>-->
        <!--<item name="android:activityOpenExitAnimation">@anim/slide_out_to_left</item>-->
        <!--<item name="android:activityCloseExitAnimation">@anim/slide_out_to_right</item>-->
        <!--<item name="android:activityCloseEnterAnimation">@anim/slide_in_from_left</item>-->
    </style>

    <!-- Base application theme. -->
    <style name="LoginTheme" parent="Theme.AppCompat.Light.DarkActionBar">

        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:windowAnimationStyle">@style/LoginTheme.Animation</item>
    </style>

    <style name="LoginTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <!--BaseAnimation-->
    <style name="LoginTheme.Animation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/screen_zoom_in</item>
        <item name="android:activityCloseExitAnimation">@anim/screen_zoom_out</item>
    </style>

    <!--首页菜单样式-->
    <style name="BottomMenu" parent="Base.TextAppearance.AppCompat.Button">
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/text_tab_style</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">11sp</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
    </style>

    <!--拍摄界面速度字体样式-->
    <style name="RecordTextxBottomMenu" parent="Base.TextAppearance.AppCompat.Button">
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/text_white_selector3</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
    </style>

    <!--titleBar样式-->
    <style name="ToolbarStyle" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="actionBarItemBackground">@drawable/toolbarItemBackground</item>
        <item name="selectableItemBackground">@drawable/toolbarItemBackground</item>
        <item name="titleTextAppearance">@style/ToolBar.Title</item>
        <item name="subtitleTextAppearance">@style/Toolbar.SubTitle</item>
        <item name="android:windowNoTitle">true</item>
        <item name="overlapAnchor">false</item>
    </style>

    <!--titlebar 标题-->
    <style name="ToolBar.Title" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/common_h1</item>
    </style>

    <!--titlebar 副标题-->
    <style name="Toolbar.SubTitle" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#99FFFFFF</item>
    </style>

    <style name="tab_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
    </style>

    <!--弹窗的点击外部可关闭的Dialog样式-->
    <style name="SpinKitViewSaveFileDialogAnimation" parent="android:style/Theme.Dialog">
        <!-- 是否有标题-->
        <item name="android:windowNoTitle">true</item>

        <!--背景颜色及透明程度-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsFloating">true</item>
        <!--是否模糊-->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
        <item name="android:windowAnimationStyle">@style/HomeItemPopupAnimation</item>
    </style>

